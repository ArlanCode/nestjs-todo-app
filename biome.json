{
	"formatter": {
		"enabled": true
	},

	"organizeImports": {
		"enabled": true
	},

	"javascript": {
		"formatter": {
			"quoteStyle": "single"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	},

	"linter": {
		"enabled": true,
		"rules": {
			"style": {
				"useImportType": "off"
			},
			"recommended": true
		}
	}
}
